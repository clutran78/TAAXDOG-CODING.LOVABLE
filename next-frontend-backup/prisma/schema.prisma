// AML/CTF Transaction Monitoring model
model AMLTransactionMonitoring {
  id                  String              @id @default(uuid())
  userId              String
  transactionId       String?             @db.Uuid
  monitoringType      AMLMonitoringType
  riskScore           Decimal             @db.Decimal(3, 2)
  riskFactors         String[]
  amount              Decimal             @db.Decimal(15, 2)
  currency            String              @default("AUD") @db.VarChar(3)
  
  // Pattern detection
  patternType         String?             @db.VarChar(100)
  patternDetails      Json?
  velocityScore       Decimal?            @db.Decimal(3, 2)
  
  // Compliance actions
  requiresReview      Boolean             @default(false)
  reviewedBy          String?
  reviewedAt          DateTime?
  reportedToAUSTRAC   Boolean             @default(false)
  reportReference     String?             @db.VarChar(255)
  reportedAt          DateTime?
  
  // Metadata
  falsePositive       Boolean             @default(false)
  notes               String?             @db.Text
  createdAt           DateTime            @default(now())
  updatedAt           DateTime            @updatedAt
  
  // Relations
  user                User                @relation(fields: [userId], references: [id], onDelete: Restrict)
  
  @@index([userId])
  @@index([transactionId])
  @@index([requiresReview])
  @@index([riskScore])
  @@index([createdAt])
  @@map("aml_transaction_monitoring")
}

// Privacy Consent Management model
model PrivacyConsent {
  id                  String              @id @default(uuid())
  userId              String
  consentType         ConsentType
  consentVersion      String              @db.VarChar(20)
  consentStatus       ConsentStatus       @default(PENDING)
  consentDate         DateTime?
  expiryDate          DateTime?
  
  // Consent details
  purposes            String[]
  dataCategories      String[]
  thirdParties        String[]
  
  // Withdrawal tracking
  withdrawnAt         DateTime?
  withdrawalReason    String?             @db.Text
  
  // Legal basis
  legalBasis          String              @db.VarChar(100)
  jurisdiction        String              @default("AU") @db.VarChar(10)
  
  // Audit
  ipAddress           String              @db.VarChar(45)
  userAgent           String?             @db.Text
  createdAt           DateTime            @default(now())
  updatedAt           DateTime            @updatedAt
  
  // Relations
  user                User                @relation(fields: [userId], references: [id], onDelete: Cascade)
  
  @@unique([userId, consentType, consentVersion])
  @@index([userId])
  @@index([consentType])
  @@index([consentStatus])
  @@index([expiryDate])
  @@map("privacy_consents")
}

// Data Access Requests (Privacy Act compliance)
model DataAccessRequest {
  id                  String              @id @default(uuid())
  userId              String
  requestType         DataRequestType
  requestStatus       DataRequestStatus   @default(PENDING)
  
  // Request details
  requestDate         DateTime            @default(now())
  requestDetails      Json?
  verificationMethod  String?             @db.VarChar(100)
  verifiedAt          DateTime?
  
  // Processing
  processedBy         String?
  processedAt         DateTime?
  completedAt         DateTime?
  
  // Response
  responseMethod      String?             @db.VarChar(100)
  responseUrl         String?             @db.Text
  responseExpiryDate  DateTime?
  
  // Compliance
  dueDate             DateTime            // 30 days from request
  extensionReason     String?             @db.Text
  extendedDueDate     DateTime?
  
  // Audit
  notes               String?             @db.Text
  createdAt           DateTime            @default(now())
  updatedAt           DateTime            @updatedAt
  
  // Relations
  user                User                @relation(fields: [userId], references: [id], onDelete: Restrict)
  
  @@index([userId])
  @@index([requestType])
  @@index([requestStatus])
  @@index([dueDate])
  @@map("data_access_requests")
}

// APRA Incident Reporting model
model APRAIncidentReport {
  id                  String              @id @default(uuid())
  incidentType        IncidentType
  severity            IncidentSeverity
  status              IncidentStatus      @default(OPEN)
  
  // Incident details
  title               String              @db.VarChar(500)
  description         String              @db.Text
  detectedAt          DateTime
  reportedAt          DateTime?
  resolvedAt          DateTime?
  
  // Impact assessment
  affectedUsers       Int                 @default(0)
  affectedSystems     String[]
  dataCompromised     Boolean             @default(false)
  financialImpact     Decimal?            @db.Decimal(15, 2)
  
  // Response actions
  immediateActions    Json?
  rootCause           String?             @db.Text
  remediation         Json?
  preventiveMeasures  Json?
  
  // Regulatory reporting
  reportedToAPRA      Boolean             @default(false)
  apraReference       String?             @db.VarChar(255)
  reportedToOAIC      Boolean             @default(false)
  oaicReference       String?             @db.VarChar(255)
  
  // Business continuity
  bcpActivated        Boolean             @default(false)
  serviceDowntime     Int?                // minutes
  dataRecoveryTime    Int?                // minutes
  
  // Metadata
  reportedBy          String?
  responsibleTeam     String?             @db.VarChar(100)
  createdAt           DateTime            @default(now())
  updatedAt           DateTime            @updatedAt
  
  @@index([incidentType])
  @@index([severity])
  @@index([status])
  @@index([detectedAt])
  @@map("apra_incident_reports")
}

// GST Transaction Details model
model GSTTransactionDetail {
  id                  String              @id @default(uuid())
  transactionId       String              @db.Uuid
  invoiceId           String?             @db.Uuid
  
  // GST calculation details
  baseAmount          Decimal             @db.Decimal(15, 2)
  gstRate             Decimal             @default(10.00) @db.Decimal(5, 2)
  gstAmount           Decimal             @db.Decimal(15, 2)
  totalAmount         Decimal             @db.Decimal(15, 2)
  
  // Tax categorization
  taxCategory         String              @db.VarChar(100)
  gstTreatment        GSTTreatment
  inputTaxCredit      Boolean             @default(false)
  
  // Business details
  supplierABN         String?             @db.VarChar(20)
  supplierName        String?             @db.VarChar(255)
  isGSTRegistered     Boolean             @default(true)
  
  // ATO reporting
  basReportingCode    String?             @db.VarChar(10)
  taxPeriod           String              @db.VarChar(10) // YYYY-MM
  reportedInBAS       Boolean             @default(false)
  basReference        String?             @db.VarChar(255)
  
  // Validation
  validated           Boolean             @default(false)
  validationErrors    String[]
  
  createdAt           DateTime            @default(now())
  updatedAt           DateTime            @updatedAt
  
  @@index([transactionId])
  @@index([invoiceId])
  @@index([taxPeriod])
  @@index([taxCategory])
  @@map("gst_transaction_details")
}

// Compliance Configuration model
model ComplianceConfiguration {
  id                  String              @id @default(uuid())
  configType          String              @unique @db.VarChar(100)
  configData          Json
  isActive            Boolean             @default(true)
  
  // AML/CTF thresholds
  amlThresholds       Json?
  
  // Privacy settings
  dataRetentionDays   Int?
  consentExpireDays   Int?
  
  // APRA requirements
  incidentReportHours Int?                @default(72)
  backupFrequency     String?             @db.VarChar(50)
  
  // GST settings
  gstRate             Decimal?            @default(10.00) @db.Decimal(5, 2)
  basReportingCodes   Json?
  
  createdAt           DateTime            @default(now())
  updatedAt           DateTime            @updatedAt
  
  @@map("compliance_configuration")
}

