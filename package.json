{
  "name": "taaxdog",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "node server.js",
    "dev:turbo": "next dev --turbo",
    "dev:clean": "rm -rf .next && npm run dev",
    "build": "prisma generate && next build",
    "build:polyfills": "prisma generate && node scripts/build-with-polyfills.js",
    "build:do": "npm run build:polyfills",
    "build:analyze": "ANALYZE=true npm run build",
    "build:clean": "rm -rf .next && npm run build",
    "build:docker": "docker build -t taaxdog .",
    "analyze-bundle": "ts-node --project tsconfig.node.json scripts/analyze-bundle.ts",
    "optimization:report": "ts-node --project tsconfig.node.json scripts/optimization-report.ts",
    "start": "NODE_ENV=production node server.js",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,yml,yaml,css,scss}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md,yml,yaml,css,scss}\"",
    "type-check": "tsc --noEmit",
    "validate": "npm run type-check && npm run lint && npm run format:check",
    "fix:naming": "node scripts/fix-naming-conventions.js",
    "fix:console": "node scripts/remove-console-statements.js",
    "quality:report": "ts-node --project tsconfig.node.json scripts/code-quality-report.ts",
    "quality:check": "npm run validate && npm run quality:report",
    "fix:all": "npm run fix:naming && npm run fix:console && npm run lint:fix && npm run format",
    "postinstall": "prisma generate",
    "build:production": "./scripts/build-production.sh",
    "precommit": "npm run lint:fix && npm run type-check",
    "prepush": "npm run test",
    "// Auth Testing Scripts": "",
    "auth:test": "ts-node --project tsconfig.node.json scripts/test-auth.ts",
    "auth:test:login": "ts-node --project tsconfig.node.json scripts/test-auth-login.ts",
    "auth:test:registration": "ts-node --project tsconfig.node.json scripts/test-auth-registration.ts",
    "auth:test:password-reset": "ts-node --project tsconfig.node.json scripts/test-password-reset.ts",
    "auth:test:session": "ts-node --project tsconfig.node.json scripts/test-auth-session.ts",
    "auth:test:oauth": "ts-node --project tsconfig.node.json scripts/test-oauth-flow.ts",
    "auth:test:2fa": "ts-node --project tsconfig.node.json scripts/test-2fa.ts",
    "auth:test:all": "npm run auth:test && npm run auth:test:password-reset && npm run auth:test:session",
    "auth:clear-rate-limits": "ts-node --project tsconfig.node.json scripts/clear-rate-limits.ts",
    "// Database Management Scripts": "",
    "db:setup": "prisma db push && prisma db seed",
    "db:reset": "prisma db push --force-reset && prisma db seed",
    "db:migrate": "prisma migrate deploy",
    "db:migrate:dev": "prisma migrate dev",
    "db:migrate:create": "prisma migrate dev --create-only",
    "db:migrate:status": "prisma migrate status",
    "db:migrate:resolve": "prisma migrate resolve",
    "db:studio": "prisma studio",
    "db:generate": "prisma generate",
    "db:format": "prisma format",
    "db:validate": "prisma validate",
    "db:seed": "ts-node --project tsconfig.node.json prisma/seed.ts",
    "db:test": "ts-node --project tsconfig.node.json scripts/test-db-connection.ts",
    "db:health": "ts-node --project tsconfig.node.json scripts/db-health-check.ts",
    "db:performance": "ts-node --project tsconfig.node.json scripts/test-database-performance.ts",
    "db:optimize": "ts-node --project tsconfig.node.json scripts/db-optimize.ts",
    "db:backup": "tsx scripts/utils/backup-manager.ts",
    "db:restore": "tsx scripts/utils/backup-manager.ts restore",
    "db:import": "npm run postgres:import-optimized",
    "db:export": "ts-node --project tsconfig.node.json scripts/db-export.ts",
    "db:clean": "ts-node --project tsconfig.node.json scripts/db-cleanup.ts",
