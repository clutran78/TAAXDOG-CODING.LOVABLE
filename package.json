    "env:validate:required": "ts-node --project tsconfig.node.json scripts/validate-required-env.ts",
    "env:validate:types": "ts-node --project tsconfig.node.json scripts/validate-env-types.ts",
    "env:generate:example": "ts-node --project tsconfig.node.json scripts/generate-env-example.ts",
    "env:encrypt": "ts-node --project tsconfig.node.json scripts/encrypt-env.ts",
    "env:decrypt": "ts-node --project tsconfig.node.json scripts/decrypt-env.ts",
    "env:dev": "node scripts/env-manager.js set development",
    "env:prod": "node scripts/env-manager.js set production",
    "env:staging": "node scripts/env-manager.js set staging",
    "env:status": "node scripts/env-manager.js status",
    "env:help": "node scripts/env-manager.js help",
